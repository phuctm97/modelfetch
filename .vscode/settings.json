{
  "json.validate.enable": false,
  "deno.enable": false,
  "nxConsole.commonNxCommands": ["run-many", "run", "reset"],
  "nxConsole.generatorAllowlist": ["nx-10x:*"],
  "nxConsole.projectViewingStyle": "tree",
  "nxConsole.generateAiAgentRules": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "tailwindCSS.experimental.configFile": {
    "apps/modelfetch-website/app/layout.css": "apps/modelfetch-website/**"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": { "source.fixAll": "always" },
  "editor.formatOnSave": true,
  "files.trimTrailingWhitespace": true,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "files.associations": {
    ".nxignore": "ignore",
    ".funcignore": "ignore",
    "*.css": "tailwindcss",
    "*.template": "ejs",
    "**/.husky/*": "shellscript"
  },
  "files.exclude": {
    "pnpm-lock.yaml": true,
    ".nvmrc": true,
    ".nx/cache": true,
    ".nx/workspace-data": true,
    ".conductor": true,
    ".husky/_": true,
    ".github/instructions/nx.instructions.md": true,
    ".cursor/rules/nx-rules.mdc": true,
    "**/*.tsbuildinfo": true,
    "**/node_modules": true,
    "**/dist": true,
    "**/.source": true,
    "**/next-env.d.ts": true,
    "**/.next": true,
    "**/worker-configuration.d.ts": true,
    "**/worker-configuration.d.ts.*": true,
    "**/.wrangler": true,
    "**/cdk.out": true,
    "**/.netlify": true,
    "**/.branches": true,
    "**/.temp": true,
    "apps/example-netlify-*/apps": true,
    "apps/example-fastly-*/bin": true,
    "apps/example-fastly-*/pkg": true
  }
}
